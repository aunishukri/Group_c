{
  "name": "restful-api-game",
  "version": "1.0.0",
  "description": "restful api game that uses POSTMAN to play",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "setupFiles": [
      "./__test__/lib/jest-setup.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=Group_c* nodemon index.js",
    "start-db": "mkdir -p ./db && mongod --dbpath ./db",
    "stop-db": "killall mongod",
    "test": "jest --runInBand",
    "test:exit": "jest --forceExit",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:test": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aunishukri/Group_c.git"
  },
  "author": "Auni,Minli,Liyana,Feerza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aunishukri/Group_c/issues"
  },
  "homepage": "https://github.com/aunishukri/Group_c#readme",
  "dependencies": {
    "@babel/highlight": "^7.24.2",
    "@jridgewell/gen-mapping": "^0.3.5",
    "@jridgewell/trace-mapping": "^0.3.25",
    "bcrypt": "^5.1.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "cord": "^1.0.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.4.5",
    "eslint": "^9.1.1",
    "express": "^4.19.2",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "picocolors": "^1.0.0",
    "superagent": "^9.0.1"
  }
}
